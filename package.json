{
  "name": "react-vite-ts",
  "private": true,
  "version": "0.1.1",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite",
    "server": "node server/server.js",
    "api": "tsx server/api/app.ts",
    "api:dev": "tsx watch server/api/app.ts",
    "api:test": "tsx server/api/test/api-test.ts",
    "dev:full": "concurrently \"npm run dev\" \"npm run server\"",
    "dev:api": "concurrently \"npm run dev\" \"npm run api:dev\"",
    "prebuild": "chmod +x node_modules/.bin/vite node_modules/.bin/tsc 2>/dev/null || true",
    "build:server": "tsc -p tsconfig.server.json",
    "build": "npm run build:server && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "npm run api:test",
    "db:setup": "node scripts/setup-database.js",
    "db:migrate": "node scripts/setup-database.js",
    "db:seed": "node scripts/seed-data.js"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.57.0",
    "@aws-sdk/client-s3": "^3.658.1",
    "@aws-sdk/s3-request-presigner": "^3.658.1",
    "@google-cloud/vision": "^5.3.3",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "10.4.20",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.13.1",
    "postcss": "8.4.49",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "3.4.16",
    "tesseract.js": "^6.0.1",
    "typescript": "^5.7.2",
    "uuid": "^11.1.0",
    "vite": "^6.0.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.8.0",
    "@types/pdf-parse": "^1.1.5",
    "@types/pg": "^8.11.10",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/xlsx": "^0.0.35",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "compression": "^1.8.1",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "eslint": "^9.16.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "helmet": "^8.1.0",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.1",
    "tsx": "^4.20.3"
  }
}
